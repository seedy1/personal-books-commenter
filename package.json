{
  "name": "node-ts-template",
  "version": "1.0.0",
  "description": "Fastify backend project",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint --fix src",
    "test": "mocha --recursive dist/specs --exit --async-stack-traces",
    "testlinux": "NODE_ENV=test mocha --recursive dist/specs --exit --async-stack-traces",
    "gentypes": "json2ts -i src/schemas/json/ -o src/schemas/types",
    "test-coverage": "nyc --reporter=html --reporter=lcov npm run test"
  },
  "author": "Seedy Jobe",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^15.14.9",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.2.0",
    "json-schema-to-typescript": "^10.1.5",
    "mocha": "^9.2.0",
    "nyc": "^15.1.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "fastify": "^3.25.2",
    "fastify-cookie": "^5.5.0",
    "fastify-session": "^5.2.1",
    "fastify-swagger": "^4.15.0",
    "mongoose": "^6.1.3",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "typeorm": "^0.2.41"
  }
